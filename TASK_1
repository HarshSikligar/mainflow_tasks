my_list = [1, 2, 3, 4, 5]
my_list.append(6)
my_list.remove(3)
my_list[0] = 10

my_dict = {'a': 1, 'b': 2, 'c': 3}
my_dict['d'] = 4
del my_dict['b']
my_dict['a'] = 100

my_set = {1, 2, 3, 4, 5}
my_set.add(6)
my_set.remove(3)

print("Modified List:", my_list)
print("Modified Dictionary:", my_dict)
print("Modified Set:", my_set)
